%tr
  %td= link_to solist.name, solist, :id => 'link_to_data'
  %td
    = link_to edit_solist_path(solist) do
      = image_tag 'edit.gif'
      = t(:edit)
  %td
    = link_to solist, :confirm => t('delete_msg', :name => solist.name), :method => :delete do
      = image_tag 'delete.gif'
      = t(:delete)

